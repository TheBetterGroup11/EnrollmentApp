@page
@model EnrollmentApplication.Pages.ProfileModel
@inject DataAccess DataAccessService
@{
    ViewData["Title"] = "Profile";
}
<head>
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" type="text/css" href="site.css" />
</head>
<body>
    <div class="default-form">
        <h1>Profile</h1>
        <table class="styled-table1">
            <tr>
                <th>Name:</th>
                <td>@Model._fullName</td>
            </tr>
            <tr>
                <th>Student ID:</th>
                <td>@Model._studentID</td>
            </tr>
            <tr>
                <th>Grade:</th>
                <td>@Model._grade</td>
            </tr>
            <tr>
                <th>Credits Completed:</th>
                <td>@Model._credits</td>
            </tr>
        </table>
    </div>
</body>